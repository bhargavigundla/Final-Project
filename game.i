void initNonPlayers() {
    for (int i = 1; i < 128; i++) {
        shadowOAM[i].attr0 = 0;
        shadowOAM[i].attr1 = 0;
        shadowOAM[i].attr2 = 0;
    }
    hideSprites();
    switch (stage) {
        case OUTSIDE:
            break;
        case HOUSE:
            break;
        case VOLCANO:
            for (int i = 0; i < 14; i++) {
                lavaRocks[i].width = 16;
                lavaRocks[i].height = 16;
                lavaRocks[i].rdel = (i % 2 == 0) ? 2 : 3;
                lavaRocks[i].cdel = 0;
                lavaRocks[i].worldCol = (i % 2 == 0) ? rand() % 228 : (rand() % (240 - LAVAROCKSWIDTH)) + 265;
                lavaRocks[i].worldRow = rand() % (mapHeight - LAVAROCKSWIDTH);
            }
            break;
        case FOREST:
            for (int i = 0; i < 10; i++) {
                poops[i].width = 8;
                poops[i].height = 8;
                poops[i].rdel = (i % 2 == 0) ? 2 : 2;
                poops[i].cdel = 0;
                poops[i].worldCol = player.worldCol + 20;
                poops[i].worldRow = 160 + 1;
                poops[i].hide = 1;
            }
            togekiss.width = 27;
            togekiss.height = 23;
            togekiss.worldCol = 0;
            togekiss.worldRow = 0;
            break;
    }
}

void updateNonPlayers() {
    switch (stage) {
        case VOLCANO:
            for (int i = 0; i < 14; i++) {
                lavaRocks[i].worldRow = (lavaRocks[i].worldRow + lavaRocks[i].rdel) % (mapHeight - LAVAROCKSHEIGHT);
            }
        break;
        case FOREST:
            poopCount = (poopCount + 1) % 30;
            for (int i = 0; i < 10; i++) {
                if (poops[i].worldRow <= 160) {
                    poops[i].worldRow += poops[i].rdel;
                }

            }
            if (poopCount == 0) {
                for (int i = 0; i < 10 && poopCount == 0; i++) {
                    if (poops[i].worldRow > 160) {
                        poops[i].worldRow = 10;
                        poops[i].worldCol = player.worldCol + 20 + (rand() % togekiss.width);
                        poopCount += 1;
                    }
                }
            }
        break;
    }
}

